package com.cilazatta.vollMed.controler;

import java.util.List;

import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.bind.annotation.RestController;

import com.cilazatta.vollMed.dto.MedicosDTO;
import com.cilazatta.vollMed.entities.CadastroDeMedicos;
import com.cilazatta.vollMed.services.MedicosService;

@RestController
@RequestMapping(value = "/medicos")
public class MedicosControler {

	private MedicosService medService;
	
	public MedicosControler(MedicosService medService) {
		this.medService = medService;
	}
	
	public MedicosDTO incluiMedico(CadastroDeMedicos medicos) {
		
	}
	
	@GetMapping
	public List<MedicosDTO> findAll() {
		return medService.findAll();
	}
	
	
	
	
}
