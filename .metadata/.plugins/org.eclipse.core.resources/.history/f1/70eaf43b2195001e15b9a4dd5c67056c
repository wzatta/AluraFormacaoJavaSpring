package com.cilazatta.vollMed.controler;


import org.springframework.web.bind.annotation.PostMapping;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.bind.annotation.RestController;

import com.cilazatta.vollMed.dto.MedicosDTO;
import com.cilazatta.vollMed.services.MedicosService;

import io.swagger.v3.oas.annotations.parameters.RequestBody;

@RestController
@RequestMapping(value = "api/v1/medicos")
public class MedicosControler {

	private MedicosService medService;
	
	public MedicosControler(MedicosService medService) {
		this.medService = medService;
	}
	
	@PostMapping
	public MedicosDTO cadastrar(@RequestBody MedicosDTO medicos) {
		System.out.println("cheguei aqui");
		return medicos;
	}

}
