package com.cilazatta.vollMed.services;

import java.util.List;

import com.cilazatta.vollMed.entities.Medicos;
import com.cilazatta.vollMed.repository.MedicosRepository;

public class MedicosService {

	private MedicosRepository medicoRepo;
	
	public MedicosService(MedicosRepository repo) {
		this.medicoRepo = repo;
	}

	public List<Medicos> findAll() {
		return medicoRepo.findAll();
	}
	
}
